{
  "name": "z-frontend-elements",
  "version": "0.1.0",
  "zenefits": {
    "level": "elements"
  },
  "main": "index.ts",
  "scripts": {
    "cy:open": "CYPRESS_COMPONENT=true zcli cypress open",
    "cy:run": "CYPRESS_COMPONENT=true zcli cypress run",
    "cy:ci": "yarn run cy:run",
    "prestart": "cd ../../ && yarn checkIntegrity",
    "build-storybook": "zrun build-storybook-storybook",
    "eslint": "zrun z-frontend-eslint",
    "lint": "zrun 'yarn tslint && yarn eslint && yarn stylelint && yarn tsCheck'",
    "test": "zrun jest",
    "tsCheck": "zrun tsc --noEmit",
    "tslint": "zrun zcli tslintPackage",
    "stylelint": "zrun z-frontend-stylelint",
    "start": "start-storybook -p $npm_package_config_port",
    "generateStorybookCsp": "zrun zcli generateStorybookCsp"
  },
  "config": {
    "port": "6007"
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "dependencies": {
    "@material-ui/core": "3.1.2",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "material-design-iconic-font": "2.2.0",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-popper": "^1.3.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.1.2",
    "react-select": "^1.2.1",
    "react-text-mask": "^5.4.3",
    "redux": "^3.7.2",
    "sanitize-html": "^1.22.0",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.7",
    "text-mask-addons": "^3.8.0",
    "z-frontend-theme": "*",
    "zbase": "*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.11",
    "@storybook/addon-knobs": "4.0.11",
    "@storybook/addon-viewport": "4.0.11",
    "@storybook/react": "4.0.11",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.123",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.12",
    "@types/react": "16.8.12",
    "@types/react-dom": "^16.9.2",
    "@types/styled-components": "^4.4.2",
    "babel-jest": "^25.2.4",
    "babel-preset-z-frontend": "*",
    "css-loader": "^3.4.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-config-z-frontend": "*",
    "faker": "^4.1.0",
    "graphql-tag": "^2.10.1",
    "jest": "^25.2.4",
    "jest-enzyme": "^4.0.1",
    "jest-styled-components": "^6.3.4",
    "react-apollo": "3.0.1",
    "react-test-renderer": "^16.10.2",
    "@testing-library/react": "^9.5.0",
    "style-loader": "^1.1.3",
    "stylelint-config-z-frontend": "*",
    "tslint": "^5.8.0",
    "typescript": "3.8.3",
    "z-frontend-app-bootstrap": "*",
    "z-frontend-cypress": "*",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-storybook-config": "*",
    "z-frontend-webpack": "*"
