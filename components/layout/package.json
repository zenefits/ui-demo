{
  "name": "z-frontend-layout",
  "version": "0.1.1",
  "zenefits": {
    "level": "layouts"
  },
  "main": "index.ts",
  "scripts": {
    "cy:open": "zcli cypress open",
    "cy:run": "zcli cypress run",
    "cy:ci": "zrun zcli startServerTest --reuse-assets",
    "prestart": "cd ../../ && yarn checkIntegrity",
    "build": "zrun build-storybook",
    "eslint": "zrun z-frontend-eslint",
    "lint": "zrun 'yarn tslint && yarn eslint && yarn stylelint && yarn tsCheck'",
    "stylelint": "zrun z-frontend-stylelint",
    "test": "zrun jest",
    "tsCheck": "zrun tsc --noEmit",
    "tslint": "zrun zcli tslintPackage",
    "start": "start-storybook -p $npm_package_config_port",
    "generateStorybookCsp": "zrun zcli generateStorybookCsp",
    "generateAppTypes": "zrun zcli generateAppTypes"
  },
  "config": {
    "port": "6010"
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "dependencies": {
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "qs": "^6.9.1",
    "react": "^16.10.2",
    "react-apollo": "3.0.1",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1",
    "tabbable": "^4.0.0",
    "z-frontend-app-bootstrap": "*",
    "z-frontend-composites": "*",
    "z-frontend-network": "*",
    "z-frontend-drag-and-drop": "*",
    "z-frontend-elements": "*",
    "z-frontend-forms": "*",
    "z-frontend-overlays": "*",
    "z-frontend-theme": "*",
    "zbase": "*"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.0.1",
    "@storybook/addon-actions": "4.0.11",
    "@storybook/addon-knobs": "4.0.11",
    "@storybook/addon-viewport": "4.0.11",
    "@storybook/react": "4.0.11",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.12",
    "@types/react": "16.8.12",
    "@types/react-dom": "^16.9.2",
    "@types/styled-components": "^4.4.2",
    "babel-jest": "^25.2.4",
    "babel-preset-z-frontend": "*",
    "enzyme": "^3.6.0",
    "eslint-config-z-frontend": "*",
    "faker": "^4.1.0",
    "fetch-mock": "^9.1.1",
    "jest": "^25.2.4",
    "@testing-library/jest-dom": "^4.2.4",
    "jest-enzyme": "^4.0.1",
    "jest-styled-components": "^6.3.4",
    "react-test-renderer": "^16.10.2",
    "@testing-library/react": "^9.5.0",
    "stylelint-config-z-frontend": "*",
    "tslint": "^5.8.0",
    "typescript": "3.8.3",
    "z-frontend-cypress": "*",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-storybook-config": "*",
    "z-frontend-webpack": "*",
    "z-frontend-yp-schema": "*"
  },
  "sideEffects": "**/!(*.js|*.ts|*.tsx|*.jsx)"
}
