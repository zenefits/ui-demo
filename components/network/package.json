{
  "name": "z-frontend-network",
  "version": "0.1.0",
  "zenefits": {
    "level": "composites"
  },
  "main": "index.ts",
  "scripts": {
    "cy:open": "CYPRESS_COMPONENT=true zcli cypress open",
    "cy:run": "CYPRESS_COMPONENT=true zcli cypress run",
    "cy:ci": "yarn run cy:run",
    "prestart": "cd ../../ && yarn checkIntegrity",
    "build-storybook": "zrun build-storybook",
    "eslint": "zrun z-frontend-eslint",
    "lint": "zrun 'yarn tslint && yarn eslint && yarn stylelint && yarn tsCheck'",
    "stylelint": "zrun z-frontend-stylelint",
    "test": "zrun jest",
    "tsCheck": "zrun tsc --noEmit",
    "tslint": "zrun zcli tslintPackage",
    "start": "start-storybook -p $npm_package_config_port",
    "generateStorybookCsp": "zrun zcli generateStorybookCsp",
    "generateAppTypes": "zrun zcli generateAppTypes"
  },
  "config": {
    "port": "6012"
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "dependencies": {
    "apollo-cache": "^1.3.2",
    "graphql": "0.12.3",
    "graphql-tag": "^2.10.1",
    "react": "^16.10.2",
    "react-apollo": "3.0.1",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1",
    "zbase": "*",
    "z-frontend-app-bootstrap": "*",
    "z-frontend-composites": "*",
    "z-frontend-elements": "*",
    "z-frontend-theme": "*"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.0.1",
    "@storybook/addon-actions": "4.0.11",
    "@storybook/addon-knobs": "4.0.11",
    "@storybook/addon-viewport": "4.0.11",
    "@storybook/react": "4.0.11",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.12",
    "@types/react": "16.8.12",
    "@types/react-is": "^16.7.1",
    "@types/react-dom": "^16.9.2",
    "@types/styled-components": "^4.4.2",
    "@types/yup": "^0.26.16",
    "apollo-client": "2.6.4",
    "babel-jest": "^25.2.4",
    "babel-preset-z-frontend": "*",
    "enzyme": "^3.6.0",
    "eslint-config-z-frontend": "*",
    "jest": "^25.2.4",
    "@testing-library/jest-dom": "^4.2.4",
    "jest-enzyme": "^4.0.1",
    "jest-styled-components": "^6.3.4",
    "react-test-renderer": "^16.10.2",
    "@testing-library/react": "^9.5.0",
    "stylelint-config-z-frontend": "*",
    "tslint": "^5.8.0",
    "tslint-config-z-frontend": "*",
    "typescript": "3.8.3",
    "z-frontend-cypress": "*",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-storybook-config": "*",
    "z-frontend-webpack": "*"
  },
  "sideEffects": "**/!(*.js|*.ts|*.tsx|*.jsx)"
}
