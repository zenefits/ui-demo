{
  "name": "z-styleguide",
  "version": "0.2.0",
  "zenefits": {
    "level": "app",
    "uiReviewer": "wachunga",
    "owner": "wachunga"
  },
  "scripts": {
    "cy:open": "zcli cypress open",
    "cy:run": "zcli cypress run",
    "cy:ci": "NODE_OPTIONS=\"--max-old-space-size=4096\" zrun zcli startServerTest --reuse-assets --single",
    "start": "styleguidist server --open",
    "prebuild": "zrun node config/buildComponentStatus",
    "build": "zrun styleguidist build",
    "deploy": "EMBER_CLI_DEPLOY_REUSE_BUILD=true ember deploy production --verbose",
    "master-only-activate": "ember deploy:activate production --revision \"${BUILD_SHA:-$TRAVIS_COMMIT}\"",
    "lint": "zrun 'yarn tslint && yarn eslint && yarn stylelint && yarn tsCheck && yarn configCheck'",
    "stylelint": "zrun z-frontend-stylelint",
    "tsCheck": "zrun tsc --noEmit",
    "configCheck": "zrun node config/validateStyleguideConfig",
    "tslint": "zrun zcli tslintPackage",
    "eslint": "zrun z-frontend-eslint",
    "test": "zrun jest",
    "storybook": "start-storybook",
    "storybook:build": "zrun build-storybook"
  },
  "config": {
    "port": "6060"
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "react": "^16.10.2",
    "react-docgen-typescript": "^1.16.2",
    "react-dom": "^16.10.2",
    "react-intl": "^2.3.0",
    "react-router-dom": "^5.1.2",
    "typescript": "3.8.3",
    "z-frontend-app-bootstrap": "*",
    "z-frontend-charts": "*",
    "z-frontend-chat": "*",
    "z-frontend-composites": "*",
    "z-frontend-data-manager": "*",
    "z-frontend-drag-and-drop": "*",
    "z-frontend-elements": "*",
    "z-frontend-forms": "*",
    "z-frontend-layout": "*",
    "z-frontend-tables": "*",
    "z-frontend-theme": "*",
    "zbase": "*"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^10.12.12",
    "@types/react": "16.8.12",
    "@types/react-dom": "^16.9.2",
    "babel-jest": "^25.2.4",
    "babel-preset-z-frontend": "*",
    "ember-cli": "ember-cli/ember-cli#master",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-zenefits": "git+ssh://git@github.com/zenefits/ember-cli-deploy-zenefits.git#v1.3.2",
    "eslint-config-z-frontend": "*",
    "jest": "^25.2.4",
    "jest-enzyme": "^4.0.1",
    "react-hot-loader": "4.12.19",
    "react-styleguidist": "^10.6.2",
    "stylelint-config-z-frontend": "*",
    "tslint": "^5.8.0",
    "typescript": "3.8.3",
    "z-frontend-cypress": "*",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-storybook-config": "*",
    "z-frontend-webpack": "*"
  }
}
