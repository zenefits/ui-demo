{
  "name": "example-server",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "PORT=8081 nodemon -x 'ts-node -r dotenv/config' -e ts,graphql src/index.ts",
    "debug": "nodemon -x 'ts-node -r dotenv/config --inspect' -e ts,graphql src/index.ts",
    "prepare": "graphql prepare",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "deploy": "now --public --dotenv .env.prod && now alias && now rm --yes --safe example-graphcool-server",
    "now-start": "node dist"
  },
  "dependencies": {
    "auth0": "^2.9.1",
    "bcryptjs": "^2.4.3",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "graphcool-binding": "^1.3.3",
    "graphql-yoga": "^1.1.5",
    "jsonwebtoken": "^8.1.0",
    "jwks-rsa": "^1.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "dotenv": "^4.0.0",
    "graphql-cli": "^2.10.0",
    "nodemon": "^1.14.8",
    "now": "^9.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.1"
  },
  "now": {
    "alias": "example-graphcool-server"
  }
}
