{
  "name": "z-frontend",
  "version": "0.1.5",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/register": "^7.8.3",
    "aws-sdk": "^2.611.0",
    "chalk": "^3.0.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "signalfx": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@types/graphql": "^0.12.3",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.12",
    "@types/node-fetch": "^2.5.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-z-frontend": "*",
    "commander": "^4.1.1",
    "cypress": "^4.1.0",
    "eslint": "^6.4.0",
    "eslint-config-z-frontend": "*",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "find-up": "^4.1.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "inquirer": "7.1.0",
    "jest": "^25.2.4",
    "lerna": "^3.16.4",
    "lint-staged": "^10.0.7",
    "npm-check": "^5.9.2",
    "prettier": "^1.19.1",
    "semver": "^7.3.2",
    "ts-jest": "^25.3.0",
    "tslint": "^5.8.0",
    "typescript": "3.8.3",
    "z-frontend-scripts": "*"
  },
  "scripts": {
    "checkIntegrity": "node src/scripts/integrityCheck.js --check",
    "preprettier": "yarn checkIntegrity",
    "prelint": "yarn checkIntegrity",
    "preinstall": "node ./src/scripts/checkLernaUpgrade.js && node ./src/scripts/checkEngines.js && node ./src/scripts/checkIfUsingYarn.js",
    "postinstall": "node src/scripts/integrityCheck.js --write",
    "prettier": "prettier --write",
    "prettify": "yarn run prettier '{{src,apps,components,tools}/**,.}/*.{js,jsx,ts,tsx,graphql,json,css,md}'",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' ./src/**/*.{js,jsx}",
    "test": "zrun jest",
    "testCoverage": "zcli testCoverage",
    "securityAudit": "NODE_OPTIONS=\"--max-old-space-size=4096\" zcli securityAudit",
    "notifyChromaticSkip": "./ci/notify_chromatic_skip.sh",
    "zcli": "zcli",
    "prettierCheck": "yarn run prettify --list-different --write=false",
    "clean": "lerna clean --yes && rm -rf ./node_modules/ ./*/*/dist ./*/*/storybook-static ./*/*/.tmp",
    "buildDependencyGraph": "node src/scripts/buildDependencyGraph.js",
    "buildDependencyGraph:png": "yarn --silent buildDependencyGraph | circo -T png > dependencyGraph.png"
  },
  "lint-staged": {
    "*": [
      "yarn checkIntegrity"
    ],
    "*.{ts,tsx}": [
      "yarn zcli runForStagedFiles --script tsCheck"
    ],
    "*.{js,ts,tsx}": [
      "yarn zcli runForStagedFiles --script 'eslint --fix' --pass-files-to-script"
    ],
    "*.{js,jsx,ts,tsx,graphql,json,css,md}": [
      "yarn run prettier"
    ],
    "tools/csp/**": [
      "lerna run generateStorybookCsp -- --stageChanges",
      "git add components/*/.storybook/preview-head.html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "workspaces": [
    "apps/*",
    "apps/hr/*",
    "apps/time/*",
    "components/*",
    "components/types/*",
    "services/*",
    "tools/*",
    "ci"
  ],
  "resolutions": {
    "graphql": "0.13.2",
    "eslint-plugin-react": "^7.14.3",
    "base64-url": "2.2.0",
    "node-sass": "git://github.com/zenefits/node-sass-empty.git#4.13.1",
    "webpack": "4.26.1",
    "jest": "^25.2.4",
    "handlebars": "4.3.1"
  }
}
