{
  "name": "z-frontend-storybook-config",
  "description": "Common config for z-frontend storybooks",
  "version": "0.1.0",
  "zenefits": {
    "level": "tool"
  },
  "private": true,
  "engines": {
    "yarn": "^1.17.3",
    "node": "^10.14.1"
  },
  "main": "index.ts",
  "scripts": {
    "eslint": "z-frontend-eslint",
    "lint": "zrun 'yarn tslint && yarn tsCheck'",
    "tsCheck": "zrun tsc --noEmit",
    "tslint": "zrun zcli tslintPackage",
    "test": "echo 'no tests'"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "z-frontend-app-bootstrap": "*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.11",
    "@storybook/addon-knobs": "4.0.11",
    "@storybook/addon-options": "4.0.11",
    "@storybook/addon-storysource": "4.0.11",
    "@storybook/addon-viewport": "4.0.11",
    "@storybook/react": "4.0.11",
    "@types/faker": "^4.1.11",
    "@types/react": "16.8.12",
    "@types/react-dom": "^16.9.2",
    "@types/react-intl": "^2.3.1",
    "@types/react-router-dom": "^5.1.3",
    "react": "^16.10.2",
    "babel-jest": "^25.2.4",
    "jest": "^25.2.4",
    "react-apollo": "3.0.1",
    "react-dom": "^16.10.2",
    "react-intl": "^2.3.0",
    "react-router-dom": "^5.1.2",
    "storybook-chromatic": "^1.4.0",
    "tslint": "^5.8.0",
    "typescript": "3.8.3",
    "webfontloader": "^1.6.28",
    "z-frontend-jest": "*",
    "z-frontend-scripts": "*",
    "z-frontend-theme": "*",
    "z-frontend-yp-schema": "*",
    "zbase": "*"
  }
